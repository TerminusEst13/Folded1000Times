        XDB3 JJJ 1 A_SpawnItem("BloodShower3",0)
        XDB3 J 0 A_CheckFloor("Spawned")
        XDB3 KKK 1 A_SpawnItem("BloodShower3",0)
        XDB3 K 0 A_CheckFloor("Spawned")
        XDB3 LLL 1 A_SpawnItem("BloodShower3",0)
        XDB3 L 0 A_CheckFloor("Spawned")
        XDB3 MMM 1 A_SpawnItem("BloodShower3",0)
        XDB3 M 0 A_CheckFloor("Spawned")
        XDB3 NNN 1 A_SpawnItem("BloodShower3",0)
        XDB3 N 0 A_CheckFloor("Spawned")
        goto Spawn+3
    Spawned:
        XDB3 M 0 A_SpawnItem("BloodShower4",0,0,0,1)
        XDB3 M 0 A_CustomMissile("EnemySplashGus1",0,0,random(0,360),2,random(0,360))
        XDB3 I 0 A_Jump(200,3)
        XDB3 I 1 A_PlaySound("S_Blodd")
        XDB3 I 0 ThrustThingZ(0,random(10,20),0,0)
        goto Spawn+3
        XDB3 I 0 A_PlaySound("S_Blodd")
        XDB3 I 0 A_ChangeFlag("PUSHABLE",0)
        XDB3 I 0 A_Jump(96,6)
        XDB3 JKLMNJKLMN 3 A_SpawnItem("BloodShower4",0,0,0,1)
    Spawned2:    
        XDB3 O 3 A_Look
        goto Spawned2
    See:    
        XDB3 O 3 A_Chase
        loop
    Melee:
        XDB3 I 1 A_FaceTarget
        XDB3 I 0 damageThing(1)
        XDB3 I 0 A_ChangeFlag("PUSHABLE",1)
        XDB3 I 0 ThrustThingZ(0,random(15,40),0,0)
        XDB3 I 0 A_Recoil(2+random(0,8))
        goto Spawn+3
    }

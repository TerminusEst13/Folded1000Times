actor HammerLightningMedium
{
    Radius 4
    Height 4
    Projectile
    Speed 0
    RenderStyle Add
    Alpha 0.5
    Scale 0.4
    States
    {
    Spawn:
        Goto Death
    Death:
        DPLE B 0
        DPLS B 1 bright //ThrustThingZ(0, 32, 1, 0)
        DPLE B 0 A_Stop
        TNT1 A 0 A_Jump(198,"Spawn2")
        TNT1 A 0 A_PlaySoundEx("hammer/spark","soundslot5",0,0)
        Goto Spawn2
    Spawn2:
        DPLE BB 0 bright A_CustomMissile("ElecShot", -6,0, random(-180, 180), CMF_AIMDIRECTION|CMF_TRACKOWNER, random(-180, 180))
        DPLE B 2 bright A_CustomMissile("ElecShot", -6,0, random(-180, 180), CMF_AIMDIRECTION|CMF_TRACKOWNER, random(-180, 180))
        DPLE CC 0 bright A_CustomMissile("ElecShot", -6,0, random(-180, 180), CMF_AIMDIRECTION|CMF_TRACKOWNER, random(-180, 180))
        DPLE C 2 bright A_CustomMissile("ElecShot", -6,0, random(-180, 180), CMF_AIMDIRECTION|CMF_TRACKOWNER, random(-180, 180))
        DPLE DD 0 bright A_CustomMissile("ElecShot", -6,0, random(-180, 180), CMF_AIMDIRECTION|CMF_TRACKOWNER, random(-180, 180))
        DPLE D 1 bright A_CustomMissile("ElecShot", -6,0, random(-180, 180), CMF_AIMDIRECTION|CMF_TRACKOWNER, random(-180, 180))
        TNT1 A 0 A_Jump(255,"Spawn3")
        TNT1 A 0 A_Jump(255,"Spawn3")
        Goto Spawn3
    Spawn3:

actor ShockScatterArc
{
  Damage 0
  Radius 1
  Height 1
  Speed 0
  Scale 0.7
  Decal Bulletchip
  Projectile
  Alpha 0.75
  Renderstyle "Add"
  -BLOODSPLATTER
  +BLOODLESSIMPACT
  +NOTIMEFREEZE
  +NOCLIP
  +THRUACTORS
  +NOBLOCKMAP
  +NOGRAVITY

    TNT1 A 0
    TNT1 A 0
    TNT1 AAAAA 1 A_JumpIfInTargetInventory("ShockArcDie",1,"Die")
    TNT1 AAAAAAAAAA 1 A_JumpIfInTargetInventory("ShockArcDie",1,"Die")
    TNT1 AAAAAAAAAA 1 A_JumpIfInTargetInventory("ShockArcDie",1,"Die")
